    "fastify": "4.22.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.3",
    "@types/node": "20.5.9",
    "@vitest/coverage-v8": "0.34.4",
    "@vitest/ui": "0.34.4",
    "eslint": "8.48.0",
    "prisma": "5.2.0",
    "tsup": "7.2.0",
    "tsx": "3.12.8",
